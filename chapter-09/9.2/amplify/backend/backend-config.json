{
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"auth": {
		"userPoolGroups": {
			"service": "Cognito-UserPool-Groups",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "ChatApp",
					"attributes": [
						"UserPoolId",
						"AppClientIDWeb",
						"AppClientID",
						"IdentityPoolId"
					]
				}
			]
		},
		"ChatApp": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"api": {
		"ChatApp": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authChatApp"
						}
					}
				}
			}
		}
	}
}